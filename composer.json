{
    "name": "hounddog/doctrine-data-fixture-module",
    "description": "Zend Framework 2 Module that provides Doctrine Data-Fixture functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "doctrine",
        "data-fixture",
        "module",
        "zf2"
    ],
    "homepage": "http://www.doctrine-project.org/",
    "authors": [
        {
            "name": "Martin Shwalbe",
            "email": "martin.shwalbe@gmail.com"
        }
    ],
    "require": { 
        "php": "^5.6 || ^7.0",
        "doctrine/doctrine-module": "^1.2",
        "zendframework/zend-servicemanager": "^2.7.6 || ^3.1",
        "zendframework/zend-stdlib": "^2.7.7 || ^3.0.1",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-orm-module": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^3.7",
        "satooshi/php-coveralls": "^0.6",
        "squizlabs/php_codesniffer": "^1.4"   
    },
    "autoload": {
        "psr-0": {
            "DoctrineDataFixtureModule": "src/"
        }
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8181 -t public/ public/index.php",
        "test": "phpunit"
    }
}
